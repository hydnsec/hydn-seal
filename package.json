{
  "name": "hydn-seal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "hydn",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "compile": "hardhat compile",
    "node": "hardhat node --network hardhat --show-stack-traces",
    "test": "npm run test:coverage",
    "test:unit": "hardhat test --logs",
    "test:coverage": "hardhat coverage",
    "test:coverage:check": "nyc check-coverage",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:js": "eslint \"**/*.{js,ts}\"",
    "lint:sol": "solhint contracts/**/*.sol",
    "lint:fix": "npm run lint:js:fix && npm run lint:sol:fix",
    "lint:js:fix": "eslint --fix \"**/*.{js,ts}\"",
    "lint:sol:fix": "solhint --fix contracts/**/*.sol",
    "prettier": "npm run prettier:js && npm run prettier:sol",
    "prettier:write": "npm run prettier:js:write && npm run prettier:sol:write",
    "prettier:js": "prettier --check .",
    "prettier:js:write": "prettier --write .",
    "prettier:sol": "prettier --check 'contracts/**/*.sol'",
    "prettier:sol:write": "prettier --write 'contracts/**/*.sol'",
    "task:findContract": "NODE_OPTIONS=--openssl-legacy-provider hardhat findContract",
    "script:deployProxy:localhost": "hardhat run scripts/deployProxy.ts --network localhost",
    "script:upgradeProxy:localhost": "hardhat run scripts/upgradeProxy.ts --network localhost",
    "task:createSeal:localhost": "hardhat createSeal --network localhost",
    "task:getNextId:localhost": "hardhat getNextId --network localhost",
    "task:mint:localhost": "hardhat mint --network localhost",
    "script:deployProxy:rinkeby": "hardhat run scripts/deployProxy.ts --network rinkeby",
    "task:getNextId:rinkeby": "hardhat getNextId --network rinkeby",
    "task:mint:rinkeby": "hardhat mint --network rinkeby",
    "task:createSeal:rinkeby": "hardhat createSeal --network rinkeby",
    "script:deployProxy:fuji": "hardhat run scripts/deployProxy.ts --network fuji",
    "script:deployProxy:avalanche": "hardhat run scripts/deployProxy.ts --network avalanche",
    "task:getNextId:avalanche": "hardhat getNextId --network avalanche",
    "script:upgradeProxy:rinkeby": "hardhat run scripts/upgradeProxy.ts --network rinkeby",
    "task:createSeal:avalanche": "hardhat createSeal --network avalanche",
    "task:mint:avalanche": "hardhat mint --network avalanche",
    "script:deployProxy:ethereum": "hardhat run scripts/deployProxy.ts --network ethereum",
    "script:upgradeProxy:ethereum": "hardhat run scripts/upgradeProxy.ts --network ethereum",
    "task:createSeal:ethereum": "hardhat createSeal --network ethereum",
    "task:getNextId:ethereum": "hardhat getNextId --network ethereum",
    "task:mint:ethereum": "hardhat mint --network ethereum"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "@openzeppelin/contracts": "4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/hardhat-upgrades": "^1.21.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "chai": "^4.3.6",
    "chai-ethers": "0.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.1",
    "hardhat-deploy": "^0.11.19",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-preprocessor": "^0.1.5",
    "hardhat-tracer": "^1.1.1",
    "memfs": "^3.4.9",
    "nyc": "^15.1.0",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4"
  },
  "nyc": {
    "cache": true,
    "extension": [
      ".sol"
    ],
    "temp-dir": "./coverage",
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
